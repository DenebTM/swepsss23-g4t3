{
    "name": "planthealth-frontend",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "license": "UNLICENSED",
    "dependencies": {
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@mui/lab": "^5.0.0-alpha.124",
        "@mui/material": "^5.11.15",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.2.4",
        "@types/jsdom": "^21.1.1",
        "@types/node": "^18.11.11",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "axios": "^1.3.4",
        "jose": "^4.13.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.9.0",
        "typescript": "^5.0.2",
        "universal-cookie": "^4.0.4"
    },
    "scripts": {
        "setup-husky": "cd .. && husky install frontend/.husky && cd frontend && npx husky add .husky/pre-commit \"cd frontend && npx lint-staged --config config/.lintstagedrc.json\"",
        "start": "vite serve",
        "mock": "export NODE_ENV=mock-api && vite serve",
        "lint-code": "npx eslint ./src --cache --fix --ignore-path .gitignore --ext .ts,.tsx",
        "check-types": "tsc --project tsconfig.json",
        "build": "vite build",
        "mvn-test": "yarn coverage && npx replace-in-files-cli --string='path=\"src' --replacement='path=\"frontend/src' sonar-report.xml",
        "test": "yarn check-types && vitest run --reporter default",
        "coverage": "vitest run --coverage"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-0",
        "@babel/plugin-syntax-flow": "^7.14.5",
        "@babel/plugin-transform-react-jsx": "^7.14.9",
        "@faker-js/faker": "^7.6.0",
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.0.0",
        "@vitejs/plugin-react": "^3.1.0",
        "@vitest/coverage-c8": "^0.29.8",
        "c8": "^7.12.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.37.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest-dom": "^4.0.3",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "8.0.3",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "jsdom": "^21.1.1",
        "lint-staged": "^13.0.4",
        "miragejs": "^0.1.47",
        "prettier": "2.8.7",
        "replace-in-files-cli": "^2.0.0",
        "ts-jest": "^29.0.3",
        "vite": "^4.2.1",
        "vite-plugin-html": "^3.2.0",
        "vitest": "0.29.8",
        "vitest-sonar-reporter": "^0.4.0"
    }
}
